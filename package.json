{
  "name": "dp-api-2024",
  "version": "1.0.0",
  "description": "API for DataProcessing",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest \"tests/(integration|e2e)/.*\\.test\\.js$\" --forceExit --testTimeout=30000",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration --forceExit",
    "test:e2e": "jest tests/e2e --forceExit",
    "test:all": "jest --forceExit --testTimeout=30000",
    "test:debug": "jest \"tests/(integration|e2e)/.*\\.test\\.js$\" --detectOpenHandles --forceExit --testTimeout=30000"
  },
  "jest": {
    "setupFilesAfterEnv": ["./jest.setup.js"],
    "globalTeardown": "./jest.teardown.js",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": true,
    "verbose": true,
    "forceExit": true,
    "detectOpenHandles": true
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "chart.js": "^4.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.3",
    "express-validator": "^7.0.1",
    "express-xml-bodyparser": "^0.3.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "js2xmlparser": "^4.0.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-scripts": "^5.0.1",
    "sequelize": "^6.35.1",
    "sequelize-cli": "^6.6.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "xml2js": "^0.6.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "jest": "^27.5.1",
    "sequelize-mock": "^0.10.2",
    "supertest": "^7.1.0"
  }
}
